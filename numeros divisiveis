package com.mycompany.agsfasayhfayfd;

import java.util.Scanner;

/**
 *
 * @author aluno.den
 */
public class Agsfasayhfayfd {

    public static void main(String[] args) {
        //Scanner
        Scanner sc = new Scanner(System.in);

        //variaveis
        int numero1;
        int numero2;
        System.out.println("Digite 1º número");
        numero1 = sc.nextInt();
        
        System.out.println("Digite 2º número");
        numero2 = sc.nextInt();

        //ação
        if (numero1 % numero2  == 0 || numero2 % numero1 == 0) {
            System.out.println("Numeros digitados, São divisiveis");

        } else {
            System.out.println("Numeros digitados, não são divisiveis");

        }
        sc.close();

    }}
