import java.util.Scanner;




public class Mavenproject1 {

    public static void main(String[] args) {
        
        //Scanner
        Scanner sc = new Scanner(System.in);
        
        //variaveis
        double computador = 2100;
        double mesaEscritorio = 650.50;
        int idade = 30; 
        char sexo = 'f'; 
        String codigo = "5290";
//retorno
System.out.printf("Produtos:");
System.out.printf("Computador, o preço é $%.2f %n",computador);
System.out.printf("Mesa de escritório, o preço é $%.2f %n",mesaEscritorio);
System.out.printf("Registro: %d anos de idade, código %s e sexo: %s",idade,codigo,sexo);
    }
}
